  </action>
  <action id="IntroduceTypeAlias" />
  <action id="NewElementInMenuGroup">
    <keyboard-shortcut first-keystroke="alt a" />
  </action>
  <action id="OptimizeImports">
    <keyboard-shortcut first-keystroke="ctrl alt o" />
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="QuickActionPopup" />
  <action id="RebuildGradleProject">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
  </action>
